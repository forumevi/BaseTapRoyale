# Base mainnet RPC (HTTPS)
RPC_URL=https://mainnet.base.org
# Deployed contract address
CONTRACT_ADDRESS=0xYourDeployedAddress
# Sponsor account private key (funded on Base)
SPONSOR_PRIVATE_KEY=0xYourSponsorPrivKey
# CORS allow origin for your frontend (e.g. https://your.app)
ORIGIN_ALLOW=http://localhost:8080
# Max logs scanned (higher = slower)
MAX_LOGS=10000
